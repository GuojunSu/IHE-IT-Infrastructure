<vlayout width="800px">
    <div apply="org.zkoss.bind.BindComposer" viewModel="@id('vm') @init('com.gaduo.zk.view_model.LeafClass')">
        <label value="@load(vm.uuid)" style="font-size: 14px; font-weight: bold;" />
        <button label="Download" onClick="@command('download')"/>
        <grid model="@load(vm.list)">
            <template name="model">
                <row>
                    <cell>
                        <label hflex="1" value="@load(each.key)" />
                    </cell>
                    <cell>
                        <label hflex="1" value="@load(each.value)" />
                    </cell>
                </row>
            </template>
        </grid>
    </div>
</vlayout>