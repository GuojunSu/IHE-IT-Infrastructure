<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<div>
    <grid>
        <columns>
            <column width="150px" />
            <column />
            <column width="150px" />
            <column />
        </columns>
        <rows>
            <row spans="1,3">
                <label value="homeCommunityId (O):" />
                <textbox hflex="1" value="@bind(vm.query.parameter.homeCommunityId)" />
            </row>
            <row>
                <label value="Document UniqueId"/>
                <textbox hflex="1" value="@bind(vm.query.parameter.id)" constraint="no empty" />
                <label value="AssociationTypes (R)(M)" />
                <combobox hflex="1" constraint="no empty" selectedItem='@bind(vm.query.parameter.associationTypes)'>
                    <comboitem label="HashMember" value="HashMember" />
                    <comboitem label="Signs" value="Signs" />
                    <comboitem label="RPLC" value="RPLC" />
                    <comboitem label="APND" value="APND" />
                    <comboitem label="XFRM" value="XFRM" />
                </combobox>
            </row>
        </rows>
    </grid>
</div>
